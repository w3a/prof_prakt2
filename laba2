#include <iostream>
#include <iostream.h>
#include <Windows.h>
using namespace std;

int main()
{
	int a,d;
float r,c;
SetConsoleOutputCP(1251);
cout<<"Введите a-> ";
cin>>a;
cout<<"Введите c-> ";
cin>>c;
cout<<"Введите d-> ";
cin>>d;
if ((d+a-1)!=0)
{r=(sqrt(25/c)-d+2)/(d+a-1);
	cout<<"Результат "<<r<<endl;
}
else
	cout<<"ошибка деления на 0 ";
return 0;
}
